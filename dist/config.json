{


    "src_dir":"maint_path/project/www-core/",
    "project_dir":"maint_path/project/",
    "wbm_dir":"maint_path/project/www-wbm/",

    "out_dir":"maint_path/run/build/",
    "meta_dir":"maint_path/run/meta/",
    "export_dir":"maint_path/export/",
    "documentation_dir":"maint_path/documentation/",

    "routing":[
        {
            "template":"/index.html",
            "tests":[
            ]
        }
    ],

    "scripts":{
        "strip":[],
        "requirejs":{
            "src": ["require-2.1.8.min.js"],
            "baseUrl": "/js/"
        },
        "append":{},
        "prepend":{
            "/js/vendors/go-jquery/jquery-2.0.3.min.js":["/js/vendors/go-jquery/jquery-2.0.3.min.js"],
            "/js/vendors/go-knockout/knockout-3.0.0.min.js":["/js/vendors/go-knockout/knockout-3.0.0.min.js"]
        }
    },
    "css":{
        "strip":[],
        "append":{},
        "prepend":{}
    },
    "phantomizer-html-assets": {
        "options": {
            "img_variance":{
                "\\.(png|jpeg|jpg)$":"@2x.$1"
            }
        }
    },
    "phantomizer-gm-merge": {
        "options": {
            "in_files": {
                "images/sprite.png":["images/icons/encre.png","images/icons/face.png"],
                "images/sprite@2x.png":["images/icons/encre@2x.png","images/icons/face@2x.png"]
            }
        }
    },
    "phantomizer-confess": {
        "test":{
            "options": {
                "in_request":"http://www.google.com/"
            }
        }
    }
}
